<UserControl x:Class="LiskMasterWallet.Pages.Accounts.Vote"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/Accounts/Forging/MyVotes.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="My Votes" Source="/Pages/Accounts/Forging/MyVotes.xaml" />
                <mui:Link DisplayName="Votes For Me" Source="/Pages/Accounts/Forging/VotesForMe.xaml" />
                <mui:Link DisplayName="Delegates" Source="/Pages/Accounts/Forging/Delegates.xaml" />
                <mui:Link DisplayName="Pending Votes" Source="/Pages/Accounts/Forging/PendingVotes.xaml" />
                <mui:Link DisplayName="Forging" Source="/Pages/Accounts/Forging/Forging.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>